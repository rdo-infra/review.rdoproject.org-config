[gerrit]
        defaultForceUpdate = true

[remote "DLRN"]
        url = git@alias_gh_DLRN:openstack-packages/DLRN.git
        projects = DLRN
[remote "rdopkg"]
        url = git@alias_gh_rdopkg:openstack-packages/rdopkg.git
        projects = rdopkg
[remote "rdoinfo"]
        url = git@alias_gh_rdoinfo:redhat-openstack/rdoinfo.git
        projects = rdoinfo

[remote "rdo-infra_puppet-dlrn"]
        url = git@alias_gh_rdo-infra_puppet-dlrn:rdo-infra/puppet-dlrn.git
        projects = rdo-infra/puppet-dlrn

[remote "rdo-packages-replica"]
        url = git@github.com:rdo-packages/${name}.git
        remoteNameStyle = basenameOnly
        projects = ^openstack/.+-distgit

[remote "rdo-infra-weirdo-replica"]
        url = git@github.com:rdo-infra/${name}.git
        remoteNameStyle = basenameOnly
        projects = ^rdo-infra/(ansible-role-)?weirdo.*

[remote "rdo-infra-playbooks"]
        url = git@github.com:rdo-infra/rdo-infra-playbooks.git
        projects = rdo-infra/rdo-infra-playbooks

[remote "rdo-infra-roles-replica"]
        url = git@github.com:rdo-infra/${name}.git
        remoteNameStyle = basenameOnly
        projects = rdo-infra/ansible-role-rdo.*

[remote "rdo-infra-rdobot"]
        url = git@github.com:rdo-infra/rdobot.git
        remoteNameStyle = basenameOnly
        projects = rdo-infra/rdobot

[remote "rdo-infra-ci-config"]
        url = git@github.com:rdo-infra/ci-config.git
        projects = rdo-infra/ci-config
